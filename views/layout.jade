!!! 5
html
	head
		title ctlnode
		meta charset='utf-8'
		link(rel='stylesheet', type='text/css', href='/styles/reset.css')
		link(rel='stylesheet', type='text/css', href='/styles/defaults.css')
		link(rel='stylesheet', type='text/css', href='/styles/core.css')
		script(type='text/javascript', src='/socket.io/socket.io.js')
		script(type='text/javascript', src='/scripts/jquery-2.0.3.min.js')
		script(type='text/javascript', src='/scripts/jade.js')
		script(type='text/javascript', src='/scripts/plugins.js')
	body
		.page
			header
				a.brand(href='') ctlnode
			.content
				block content
			footer
				.lang-options
					span: a(href='?language=en') en
					span: a(href='?language=de') de
					span: a(href='?language=fr') fr
					span: a(href='?language=it') it
				.listinfo
					span.alias
						| #{list.alias}
					span.creationMoment
						| #{lingua.taskinfo.creationMoment} #{list.creationMoment}
					span.expireMoment
						| #{lingua.taskinfo.expireMoment} #{list.expireMoment}
					